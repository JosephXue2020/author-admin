    <datastream id="EXTMETA" versionable="true">
        <datastreamVersion createTime="2022-05-26 16:12:56" id="1.0">
            <content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="neu:extMetaEntity">
                <extElement>
                    <name>latinname</name>
                    <value></value>
